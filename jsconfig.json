{
	"compilerOptions": {
		"plugins": [
			{
				"name": "typescript-styled-plugin",
				"lint": {
					"validProperties": [
						"margin-start",
						"margin-end",
						"margin-horizontal",
						"margin-vertical",
						"padding-start",
						"padding-end",
						"padding-vertical",
						"padding-horizontal",
						"elevation",
						"shadow-color",
						"shadow-offset",
						"shadow-opacity",
						"shadow-radius",
						"end",
						"resize-mode"
					]
				}
			}
		]
	}
}
